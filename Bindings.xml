<Bindings>
    <Binding name="Open Guildbook" category="Guildbook">
        GuildbookUI:Show()
    </Binding>
    <Binding name="Chat" category="Guildbook">
        GuildbookUI:Show()
        GuildbookUI:OpenTo("chat")
    </Binding>
